<nav>
        <div class="nav-wrapper teal darken-4">
          <a href="#" class="brand-logo center">Mongo Scraper - Saved Articles</a>
          <ul id="nav-mobile" class="left hide-on-med-and-down">
             <li><a href="/">Home</a></li>
            <li><a href="/saved">Saved Articles</a></li>
            <li><a href="/clear" class="waves-effect waves-light red darken-2 btn" id="clear-saved">Clear Articles</a></li>
          </ul>
          </ul>
        </div>
      </nav>

    <div class="jumbotron text-center ">
      <div class="overlay"></div>
      <div class="background-image"></div>
    </div>
    <div class="container-fluid article-container container" id="articles">
      {{#each SavedArticle}}
      <div data-_id="{{_id}}" class="card">
        <div class="card-header">
          <h4 class="center">
            <a
              class="article-link"
              target="_blank"
              rel="noopener noreferrer"
              href="{{link}}"
              >{{title}}</a
            >
            <a data-id="{{id}}" class="btn btn-success save btn-small" id="delete-from-saved">Delete from Saved</a>
            <a data-id="{{id}}" class="btn btn-success save btn-small" id="article-note">Article Note</a>
          </h4>
        </div>
        {{/each}}

